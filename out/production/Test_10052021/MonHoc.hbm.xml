<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.MonHoc" table="monhoc" schema="htmh">
        <id name="maMh">
            <column name="MaMH" sql-type="char(10)" length="10"/>
        </id>
        <property name="tenMh">
            <column name="TenMH" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="soTinChi">
            <column name="SoTinChi" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>