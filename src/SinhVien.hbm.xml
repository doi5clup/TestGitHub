<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.SinhVien" table="sinhvien" schema="htmh">
        <id name="maSv">
            <column name="MaSV" sql-type="char(10)" length="10"/>
        </id>
        <property name="tenSv">
            <column name="TenSV" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="gioiTinh">
            <column name="GioiTinh" sql-type="char(10)" length="10" not-null="true"/>
        </property>
        <property name="ngSinh">
            <column name="NgSinh" sql-type="date" not-null="true"/>
        </property>
        <property name="diaChi">
            <column name="DiaChi" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="pass">
            <column name="Pass" sql-type="char(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>