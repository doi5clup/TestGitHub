<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="POJO.LopHoc" table="lophoc" schema="htmh">
        <id name="maLh">
            <column name="MaLH" sql-type="char(10)" length="10"/>
        </id>
        <property name="tenLh">
            <column name="TenLH" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="hsNam">
            <column name="HSNam" sql-type="int" not-null="true"/>
        </property>
        <property name="hsNu">
            <column name="HSNu" sql-type="int" not-null="true"/>
        </property>
        <property name="tongHs">
            <column name="TongHS" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>